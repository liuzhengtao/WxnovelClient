page{
  background:#DAA520;
}
.index-header{
  height: 130px;
  width: 100%;
  border-radius: 5px;
  background: #E0FFFF;
  display: flex;
  flex-direction: row;
  padding-top: 20px;
  padding-left: 10px;
}
.one-book{
  display: flex;
  flex-direction: row;
  height: 100px;
  width: 80px;
  margin-left: 15px;
}
.one-book image{
  height: 100px;
  width: 80px;
}
.index-cd{
  height: 30px;
  background: #AEEEEE;
}
.index-section{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.add-book{
  display: flex;
  flex-direction: row;
  height: 100px;
  width: 80px;
  margin-left: 15px;
  margin-top:10px; 
  border: 3px solid black;
}
.add-book image{
  height: 100px;
  width: 80px;
}